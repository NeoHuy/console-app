{
    "name": "buonzz/console-app",
    "description": "Boilerplate project files for creating Console Applications on executable PHAR file.",
    "keywords": ["template", "phar", "package", "console application"],
    "license": "DBAD",
    "authors": [
        {
            "name": "Darwin Biler",
            "email": "buonzz@gmail.com"
        }
    ],
    "type": "project",
    "require": {
		"php": ">=5.4",
        "vlucas/phpdotenv": "~2.2",
        "symfony/console": "~3.2",
        "illuminate/database": "^5.0",
        "monolog/monolog": "^1.22",
        "guzzlehttp/guzzle": "~6.0",
        "tightenco/collect": "^5.4",
        "illuminate/queue": "^5.4",
        "illuminate/encryption": "^5.4",
        "illuminate/bus": "^5.4"
    },
	"require-dev": {
		"phpunit/phpunit": "^4.0"
    },
    "bin": ["bin/consoleapp"],
	"autoload": {
        "psr-4": {
            "App\\Console\\": "src/"
        }
    }
}
